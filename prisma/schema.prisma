generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// datasource db {
//   provider = "mongodb"
//   url      = env("DATABASE_URL")
// }

model Exception {
  id         String   @id @default(uuid()) @map("_id")
  statusCode Int
  message    String
  path       String?
  stack      String?
  createdAt  DateTime @default(now())

  @@map("tabela")
}
