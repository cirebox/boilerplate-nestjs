{
  "name": "boilerplate-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">20.0.0 <=22.14.0",
    "npm": ">=10.0.4 <=10.9.2"
  },
  "overrides": {
    "glob": "^10.3.10",
    "jackspeak": "^2.3.6",
    "minimatch": "^9.0.3",
    "path-scurry": "^1.10.1"
  },
  "scripts": {
    "type-check": "tsc -b",
    "lint": "eslint './src/**/*.ts' --fix --config eslint.config.mjs",
    "lint:check": "eslint './src/**/*.ts' --config eslint.config.mjs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migration:seed": "npx prisma generate && npx prisma db push --force-reset && ts-node prisma/seed.ts",
    "seed": "npx prisma db push --force-reset && ts-node prisma/seed.ts",
    "migration": "npx prisma generate && npx prisma db push",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docs": "npx typedoc --darkHighlightTheme dark-plus",
    "prepare": "husky"
  },
  "dependencies": {
    "@fastify/compress": "^8.0.1",
    "@fastify/static": "^8.1.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/platform-fastify": "^11.0.12",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.5.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "axios": "^1.8.4",
    "bullmq": "^5.44.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "express-basic-auth": "^1.2.1",
    "ioredis": "^5.6.0",
    "kafkajs": "^2.2.4",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.17.0",
    "winston-logstash": "^1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^11.0.12",
    "@types/amqplib": "^0.10.7",
    "@types/compression": "^1.7.5",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.1",
    "@types/jest": "29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "22.13.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "conventional-changelog-atom": "^5.0.0",
    "dotenv-safe": "^9.1.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "prisma-dbml-generator": "^0.6.0",
    "prisma-erd-generator": "^2.0.4",
    "prisma-markdown": "^1.0.9",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typedoc": "^0.27.9",
    "typedoc-github-theme": "^0.2.1",
    "typedoc-plugin-merge-modules": "^6.1.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "tsdoc": {
    "source": "{string}",
    "destination": "{string}",
    "systemName": "{string}",
    "footer": "{string}",
    "copyright": "{string}",
    "outputSourceFiles": "{boolean}",
    "commentsOnly": "{boolean}"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.prisma": [
      "npx prisma format"
    ]
  }
}
