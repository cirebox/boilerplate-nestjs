{
  "name": "boilerplate-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint './src/**/*.ts' --fix --config eslint.config.mjs",
    "lint:check": "eslint './src/**/*.ts' --config eslint.config.mjs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "migration:seed": "npx prisma generate && npx prisma db push --force-reset && ts-node prisma/seed.ts",
    "seed": "npx prisma db push --force-reset && ts-node prisma/seed.ts",
    "migration": "npx prisma generate && npx prisma db push",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docs": "npx typedoc --darkHighlightTheme dark-plus"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/swagger": "^8.0.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^5.21.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "express-basic-auth": "^1.2.1",
    "nest-winston": "^1.9.7",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.15.0",
    "winston-logstash": "^1.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@mermaid-js/mermaid-cli": "^11.2.1",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@types/amqplib": "^0.10.5",
    "@types/compression": "^1.7.5",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "22.8.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "dotenv-safe": "^9.1.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-erd-generator": "^1.11.2",
    "prisma-markdown": "^1.0.9",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typedoc": "^0.26.10",
    "typedoc-github-theme": "^0.1.2",
    "typedoc-plugin-merge-modules": "^6.0.3",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0"
  },
  "tsdoc": {
    "source": "{string}",
    "destination": "{string}",
    "systemName": "{string}",
    "footer": "{string}",
    "copyright": "{string}",
    "outputSourceFiles": "{boolean}",
    "commentsOnly": "{boolean}"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
